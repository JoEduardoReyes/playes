mixin head(title, style)
  link(rel="stylesheet", href=style)
  title=title

head
  meta(charset="UTF-8")
  meta(http-equiv="X-UA-Compatible", content="IE=edge")
  meta(name="viewport", content="width=device-width, initial-scale=1.0")
  script(src="https://kit.fontawesome.com/6c001dbf4f.js" crossorigin="anonymous")
  block headTitleStyle